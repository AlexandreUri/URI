import java.util.*; 
public class Main{ 
	public static long[] vetor=new long[65];
	 
	public static void main(String args[]){
			Scanner leitura=new Scanner(System.in);
			 int n=leitura.nextInt(),u;
			 int x[]=new int[1000005];
			 for(int i=0;i<n;i++){
			 	u=leitura.nextInt();
			 	x[u]=1;
			 }
			 int cont=0;
			 for(int i=0;i<1000005;i++){
			 	if(x[i]==1)cont++;
			 }
			 System.out.println(cont);
			}
}
