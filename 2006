import java.util.*; 
public class Main{ 
	public static long[] vetor=new long[65];
	 
	public static void main(String args[]){
			Scanner leitura=new Scanner(System.in);
			  int resp=leitura.nextInt();
			  int vet[]=new int[5];
			  int cont=0;
			  for(int i=0;i<5;i++){
			  	vet[i]=leitura.nextInt();
			  	if(vet[i]==resp)cont++;
			  }
			  System.out.println(cont);
			}
}
