import java.util.*;
import java.math.*;
public class Main{ 
	 
	public static void main(String caralho[]){
			Scanner leitura=new Scanner(System.in);
			 int volume=leitura.nextInt(),q=leitura.nextInt();
			 int y;
			 for(int i=0;i<q;i++){

				 y=leitura.nextInt();
				 if(volume+y>100)volume=100;
				 else if(volume+y<0)volume=0;
				 else volume+=y; 
			 }
			 System.out.println(volume);
	}
}
