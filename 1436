import java.util.*; 
public class Main{ 
	public static long[] vetor=new long[65];
	 
	public static void main(String args[]){
			Scanner leitura=new Scanner(System.in);
			 int n=leitura.nextInt();
			 int cont=0;
			 for(int i=0;i<n;i++){
			 	cont++;
			 	int x=leitura.nextInt();
			 	int vet[]=new int[x];
			 	for(int j=0;j<x;j++)vet[j]=leitura.nextInt();
			 	System.out.printf("Case %d: %d\n",cont,vet[x/2]);
			 }
	}
}
